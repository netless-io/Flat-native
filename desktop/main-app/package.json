        "name": "Black-Hole",
        "url": "https://bugs.cc/"
    },
    "repository": {
        "url": "https://github.com/netless-io/Flat-native"
    },
    "scripts": {
        "start": "cross-env NODE_ENV=development webpack --info-verbosity=none --config webpack/webpack.dev.js",
        "copy:renderBuildCode": "node ./scripts/Copy-render-build-code.js",
        "launch:electron": "node --trace-uncaught ./scripts/Launch-electron.js",
        "build:main": "cross-env NODE_ENV=production webpack --config webpack/webpack.prod.js",
        "build": "yarn copy:renderBuildCode && yarn build:main",
        "pack:win": "node ./scripts/Pack.js win",
        "pack:mac": "node ./scripts/Pack.js mac",
        "ship": "yarn build && node ./scripts/Pack.js",
        "ship:win": "yarn build && node ./scripts/Pack.js win",
        "ship:mac": "yarn build && node ./scripts/Pack.js mac",
        "download:agoraAddon": "node ./scripts/Download-agora-addon.js && yarn move:agoraAddon",
        "download:agoraAddon:mac": "node ./scripts/Download-agora-addon.js --arch=x64 --platform=darwin && yarn move:agoraAddon",
        "download:agoraAddon:win": "node ./scripts/Download-agora-addon.js --arch=x86 --platform=win32 && yarn move:agoraAddon",
        "move:agoraAddon": "move-cli ./build/ ./node_modules/agora-electron-sdk/build/",
        "postinstall": "yarn download:agoraAddon",
        "lint": "lint-staged"
    },
    "devDependencies": {
        "@babel/core": "^7.11.4",
        "@babel/plugin-proposal-export-default-from": "^7.12.1",
        "@babel/plugin-transform-runtime": "^7.11.0",
        "@babel/preset-env": "^7.11.0",
        "@babel/preset-typescript": "^7.10.4",
        "@types/fs-extra": "^9.0.6",
        "@types/node": "^14.14.16",
        "@types/webpack-env": "^1.15.2",
        "@typescript-eslint/eslint-plugin": "^4.11.0",
        "@typescript-eslint/parser": "^4.11.0",
        "babel-loader": "^8.1.0",
        "babel-plugin-transform-class-properties": "^6.24.1",
        "cross-env": "^7.0.2",
        "dotenv-flow-webpack": "^1.1.0",
        "electron": "11.3.0",
        "electron-builder": "^22.10.5",
        "electron-devtools-installer": "git://github.com/BlackHole1/electron-devtools-installer#fb67b54fe18bdf556fb3e3d00db5fbaf0dbb294d",
        "electron-webpack": "^2.8.2",
        "eslint": "^7.7.0",
        "eslint-config-prettier": "^6.11.0",
        "eslint-loader": "^4.0.2",
        "eslint-plugin-prettier": "^3.1.4",
        "file-loader": "^6.2.0",
        "fork-ts-checker-webpack-plugin": "^6.0.4",
        "fs-extra": "^9.0.1",
        "js-yaml": "^3.14.0",
        "lint-staged": "^10.5.4",
        "move-cli": "^1.2.1",
        "nodemon": "^2.0.4",
        "prettier": "^2.1.0",
        "rimraf": "^3.0.2",
        "thread-loader": "^2.1.3",
        "flat-types": "^0.1.0",
        "typescript": "^4.1.2",
        "webpack": "^4.44.1",
        "webpack-cli": "^3.3.12",
        "webpack-merge": "^5.7.3"
    },
    "dependencies": {
        "agora-electron-sdk": "education290",
        "jquery": "^3.5.1",
        "rxjs": "^6.6.6",
        "ts-enum-util": "^4.0.2"
    },
